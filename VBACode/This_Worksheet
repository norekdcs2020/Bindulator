Arkusz5.Range("J14").Value = "NIEWYKONANA" & Chr(10) & "NOT PERFORMED"
Arkusz5.Range("J27").Value = "NIEWYKONANA" & Chr(10) & "NOT PERFORMED"
Arkusz5.Range("J39").Value = "NIEWYKONANY" & Chr(10) & "NOT PERFORMED"

